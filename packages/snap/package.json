{
  "name": "@portal-solutions/pasqi-snap",
  "type": "module",
  "zshy": {
    "exports": {
      ".": "./index.ts",
      "./api": "./api.ts",
      "./core": "./core.ts"
    }
  },
  "license": "MPL-2.0",
  "scripts": {
    "build": "npx zshy -p ../../tsconfig.json"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "types": "./dist/index.d.cts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./api": {
      "types": "./dist/api.d.cts",
      "import": "./dist/api.js",
      "require": "./dist/api.cjs"
    },
    "./core": {
      "types": "./dist/core.d.cts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    }
  }
}
